<odoo>
        <record id="parent_school_view_list" model="ir.ui.view">
            <field name="name">parent.school.list</field>
            <field name="model">parent.school</field>
            <field name="arch" type="xml">
                <list>
                    <field name="parent_name"/>
                    <field name="phone"/>
                    <field name="email"/>
                    <field name="address"/>
                </list>
            </field>
        </record>
        <record id="parent_school_view_form" model="ir.ui.view">
            <field name="name">parent.school.form</field>
            <field name="model">parent.school</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="parent_name"/>
                            <field name="phone"/>
                            <field name="email"/>
                            <field name="address"/>
                        </group>
                        <notebook>
                            <page string="children">
                                <field name="student_ids"/>

                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="parent_action" model="ir.actions.act_window">
            <field name="name">Parent Action</field>
            <field name="res_model">parent.school</field>
            <field name="view_mode">form,list</field>
        </record>

        <menuitem id="Parent_school" name="Parents" sequence="3" action="parent_action" parent="school_menu_root"/>

</odoo>