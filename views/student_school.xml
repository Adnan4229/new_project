<odoo>
    <data>
        <record id="student_school_view_list" model="ir.ui.view">
            <field name="name">studnet.school.list</field>
            <field name="model">student.school</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                    <field name="phone"/>
                    <field name="address"/>
                    <field name="email"/>
                </list>
            </field>
        </record>

        <record id="teacher_school_view_list" model="ir.ui.view">
            <field name="name">studnet.school.form</field>
            <field name="model">student.school</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group string="Personal Information">
                            <field name="name"/>
                            <field name="roll_no"/>
                            <field name="phone"/>
                            <field name="address"/>
                            <field name="email"/>
                        </group>
                        <group string="Other Information">
                            <field name="dob"/>
                            <field name="gender"/>
                            <field name="age"/>
                            <field name="admission_date"/>
                            <field name="city"/>
                            <fieled name="state"/>
                            <field name="zip_code"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="student_action" model="ir.actions.act_window">
            <field name="name">Student Action</field>
            <field name="res_model">student.school</field>
            <field name="view_mode">list,form</field>
        </record>


        <menuitem id="school_menu_root" name="school management" sequence="1"/>
        <menuitem id="student_school" name="Students" sequence="1" action="student_action" parent="school_menu_root"/>
    </data>
</odoo>